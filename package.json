{
  "name": "twitch-js",
  "version": "2.0.0-alpha.1",
  "description": "Javascript library for the Twitch Messaging Interface.",
  "license": "MIT",
  "author": "Schmoopiie",
  "keywords": [
    "tmi",
    "twitch",
    "twitch.tv",
    "stream",
    "broadcast",
    "message",
    "messaging",
    "interface",
    "subscriber",
    "websocket",
    "ws",
    "bot",
    "robot"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/twitch-apis/twitch-js.git"
  },
  "bugs": {
    "url": "https://github.com/twitch-apis/twitch-js/issues"
  },
  "main": "lib/index.js",
  "files": [
    "dist",
    "examples",
    "lib",
    "src"
  ],
  "runkitExampleFilename": "examples/runkit.js",
  "scripts": {
    "clean": "rimraf lib dist",
    "lint": "eslint src",
    "test": "mocha --compilers js:babel-core/register --reporter spec --require should test",
    "jest": "jest",
    "coverage": "nyc report --reporter=text",
    "test:coverage": "nyc npm run test",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "prepublish": "npm run build",
    "prebuild": "npm run clean",
    "build:cjs": "babel src --out-dir lib --source-maps",
    "build:umd:dev": "webpack",
    "build:umd:prod": "cross-env NODE_ENV=PRODUCTION webpack",
    "build": "run-p build:**"
  },
  "dependencies": {
    "node-fetch": "^1.7.3",
    "request": "2.74.0",
    "ws": "1.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "8.0.3",
    "babel-jest": "^22.0.6",
    "babel-loader": "7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^14.5.0",
    "coveralls": "^3.0.0",
    "cross-env": "5.1.1",
    "eslint": "4.13.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jest": "^21.6.2",
    "fetch-mock": "^6.0.0-beta.7",
    "hook-std": "0.2.0",
    "jest": "^22.0.6",
    "mocha": "2.2.5",
    "npm-run-all": "4.1.2",
    "nyc": "^11.3.0",
    "regenerator-runtime": "^0.11.1",
    "rimraf": "2.5.2",
    "should": "7.0.4",
    "webpack": "3.10.0"
  },
  "browser": {
    "ws": false,
    "request": false,
    "node-fetch": false
  }
}
